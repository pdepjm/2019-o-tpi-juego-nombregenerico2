import actores.*

describe "test de agua" {
	const agua1 = new Agua()
	const rana1P = new Rana(nombreSprite = "rana")
	
	test "es atravesable" {
		assert.that(agua1.esAtravesable())
	}
	
	test "colisionar con una rana" {
		assert.throwsException({agua => agua.colisionarConUnaRana(rana1P)})
	}
}