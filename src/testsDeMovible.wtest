import clasesBase.*

describe "test de movible" {
	const movible = new Movible()
	
	test "empezarMovimientoConstante" {
		assert.throwsException({unMovible => unMovible.empezarMovimientoConstante()})
	}
	
	test "moverse" {
		assert.throwsException({unMovible => unMovible.moverse()})
	}
	
	test "es atravesable" {
		assert.that(movible.esAtravesable())
	}
}