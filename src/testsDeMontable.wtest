import clasesBase.*

describe "test de montable" {
	const montable = new Montable()
	
	test "empezarMovimientoConstante" {
		assert.throwsException({unMontable => unMontable.empezarMovimientoConstante()})
	}
	
	test "moverse" {
		assert.throwsException({unMontable => unMontable.moverse()})
	}
	
	test "es atravesable" {
		assert.that(montable.esAtravesable())
	}
}