import actores.*

describe "test de barrera limite" {
	const barreraLimite1 = new BarreraLimite()
	const rana1P = new Rana(nombreSprite = "rana")
	
	test "no es atravesable" {
		assert.notThat(barreraLimite1.esAtravesable())
	}
	
	test "colisionar con una rana" {
		assert.throwsException({barreraLimite => barreraLimite.colisionarConUnaRana(rana1P)})
	}
}